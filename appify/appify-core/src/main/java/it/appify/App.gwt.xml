<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.7.0//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.7.0/distro-source/core/src/gwt-module.dtd">
<module>
	<inherits name="com.google.gwt.user.User" />
	<inherits name="com.google.gwt.json.JSON" />
	<inherits name="it.appify.Presenter" />
	<inherits name="it.appify.Battery" />
	<inherits name="it.appify.Geolocation" />
	<inherits name="it.appify.Offline" />
	<inherits name="it.appify.Storage" />
	<inherits name="it.appify.ScreenOrientation" />

	<source path="app" />


	<generate-with class="it.appify.generator.WebAppGenerator">
		<when-type-assignable class="it.appify.app.WebApp" />
	</generate-with>
</module>